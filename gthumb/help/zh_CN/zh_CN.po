#: C/gthumb.xml:2826(command)
msgid ""
"rm ~/myarchive.zip; zip -j ~/myarchive %F; uuencode ~/myarchive.zip ~/"
"myarchive.zip | mail -s Photos your@emailaddress.com"
msgstr ""
"rm ~/myarchive.zip; zip -j ~/myarchive %F; uuencode ~/myarchive.zip ~/"
"myarchive.zip | mail -s Photos your@emailaddress.com"

